{
  "name": "japan-ranranru-racing-association",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "check": "npx tsc --noEmit && npx prettier --write .",
    "test": "vitest",
    "db:seed": "npx tsx src/shared/db/seed.ts",
    "db:reset": "npx tsx src/shared/db/reset.ts",
    "db:admin": "npx tsx src/shared/db/set-admin.ts",
    "db:setup": "npx drizzle-kit push && npm run db:seed",
    "d:up": "docker compose up -d",
    "d:down": "docker compose down",
    "d:restart": "docker compose restart",
    "d:build": "docker compose up -d --build",
    "d:install": "docker compose exec app npm install",
    "d:logs": "docker compose logs -f",
    "d:clean": "docker compose down -v && docker compose up -d --build"
  },
  "dependencies": {
    "@auth/core": "0.41.1",
    "@auth/drizzle-adapter": "^1.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "nextjs-toploader": "^3.9.17",
    "postgres": "^3.4.8",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "@auth/core": "0.41.1",
    "esbuild": "0.25.0"
  }
}
