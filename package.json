{
  "name": "paper-tipster-web",
  "version": "0.7.0",
  "private": true,
  "packageManager": "pnpm@10.29.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "check": "pnpm tsc --noEmit && pnpm prettier --write .",
    "test": "vitest",
    "db:seed": "pnpm tsx src/shared/db/seed.ts",
    "db:seed:master": "pnpm db:seed --master-only",
    "db:reset": "pnpm tsx src/shared/db/reset.ts",
    "db:admin": "pnpm tsx src/shared/db/set-admin.ts",
    "db:setup": "pnpm drizzle-kit push && pnpm db:seed",
    "redis:reset": "pnpm tsx src/shared/lib/reset-redis.ts",
    "p:install": "docker compose exec -T app pnpm install",
    "p:dev": "docker compose exec app pnpm dev",
    "p:build": "docker compose exec -T app pnpm build",
    "p:add": "docker compose exec -T app pnpm add",
    "d:up": "docker compose up -d",
    "d:down": "docker compose down",
    "d:restart": "docker compose restart",
    "d:build": "docker compose up -d --build",
    "d:install": "docker compose exec -T app pnpm install",
    "d:logs": "docker compose logs -f",
    "d:clean": "docker compose down -v && docker compose up -d --build",
    "d:test": "docker compose exec -T app pnpm test",
    "d:check": "docker compose exec -T app pnpm check",
    "lint:fix": "eslint --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@auth/core": "0.41.1",
    "@auth/drizzle-adapter": "^1.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "ioredis": "^5.9.2",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "nextjs-toploader": "^3.9.17",
    "postgres": "^3.4.8",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "@auth/core": "0.41.1",
      "esbuild": "0.25.0"
    }
  }
}
